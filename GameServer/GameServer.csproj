    <PackageReference Include="YamlDotNet" Version="16.3.0" />
  </ItemGroup>
  
  <ItemGroup>
    <_ContentIncludedByDefault Remove="Utils\Matrix\Matrix.Core\obj\Matrix.Core.csproj.nuget.dgspec.json" />
